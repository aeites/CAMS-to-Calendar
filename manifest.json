{
  "manifest_version": 2,

  "name": "CAMS-to-Calendar",
  "description": "This extension allows you to import your class Schedule in CAMS into your Google Calendar with the click of a button.",
  "version": "1.0",
	"background":
  {
	"scripts": ["thirdParty/jquery-3.2.0.js"]
  },
  "browser_action": {
          "default_icon": {      
            "16": "icon.png",          
            "24": "icon.png", 
            "32": "icon.png"   
			},
			"default_popup": "popup.html"
	},
	
	"content_security_policy": "script-src 'self' https://apis.google.com object-src 'self'",
  
	"content_scripts": [{
		"matches": ["<all_urls>","https://apis.google.com/js/client.js"],
		"styles": ["style.css"],
		"js": ["thirdParty/jquery-3.2.0.js","thirdParty/client.js","contentscript.js","calendarStuff.js"]
	}],
	
	"permissions": [
		"activeTab",
		"tabs",
		"https://ajax.googleapis.com/",
		"https://cams.floridapoly.org/*/",
		"http://*/",
		"https://*/",
		"https://www.googleapis.com/auth/calendar"
	]
}