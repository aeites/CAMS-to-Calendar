{
  "manifest_version": 2,

  "name": "CAMS-to-Calendar",
  "description": "This extension allows you to import your class Schedule in CAMS into your Google Calendar with the click of a button.",
  "version": "1.0",
  "icons": {"128" : "icon.png" },
  "content_scripts" : [
    {
     "matches": ["*://.cams.floridapoly.org/*" ],
     "exclude_matches": ["*://*/*login*"],
     "js": ["background.js"]}
   ],
  "browser_action":
  {
          "default_icon": "icon.png",
          "default_title": "CAMS-to-Calendar",
          "default_popup" : "popup.html"
  }
}
